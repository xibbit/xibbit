{
  "name": "marko-public-figure",
  "private": true,
  "version": "1.5.3",
  "description": "Public Figure webapp using Marko",
  "repository": "github:xibbit/xibbit",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.0.0",
    "js-sha256": "^0.8.0",
    "marko": "^4.24.6",
    "marko-starter": "xibbit/marko-starter#middleware"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-proxy-middleware": "^2.0.6",
    "prettier": "^2.8.0"
  },
  "scripts": {
    "build": "NODE_ENV=production marko-starter build",
    "format": "prettier src/**/*.{js,css,less} --write && marko-prettyprint src",
    "lint": "eslint src",
    "serve": "NODE_ENV=production marko-starter serve-static",
    "start": "marko-starter server",
    "test": "npm run lint"
  }
}
